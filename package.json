{
  "name": "gpt-professor",
  "version": "0.0.8",
  "description": "Tool used to create a simple CLI around ChatGPT, in order to help any kind of teaching activity",
  "main": "index.js",
  "bin": "./index.js",
  "scripts": {
    "test": "exit 0",
    "measure": "node index.js measure-generated-content file_test",
    "autograde": "node index.js autograde -c file_test/autograde2.json -r file_test -o file_test/professorgpt.json --minify --debug",
    "autograde-gemini": "node index.js autograde-gemini -c file_test/autograde2.json -r file_test -o file_test/professorgpt.json --minify --debug",
    "feedback": "node index.js feedback -c file_test/feedback.json -r git@github.com:UE-WebX/projet-maar-2024-benghachemhamzamorozova.git --minify --debug"
  },
  "keywords": [
    "professor",
    "teaching",
    "chatgpt",
    "gpt",
    "cli",
    "tool"
  ],
  "author": "Alexandre Dubois (jochy)",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^10.4.15",
    "axios": "1.6.7",
    "bard-ai": "^2.0.3",
    "bluebird": "^3.7.2",
    "commander": "^11.0.0",
    "cssnano": "^6.0.1",
    "cssnano-preset-lite": "^3.0.0",
    "dotenv": "^16.3.1",
    "gemini-ai": "^1.1.0",
    "globby": "^13.2.2",
    "googlebard": "^1.0.7",
    "jsdom": "^22.1.0",
    "minify-xml": "^4.0.0",
    "octokit": "^3.1.2",
    "openai": "^3.3.0",
    "pdf-parse": "^1.1.1",
    "postcss": "^8.4.29",
    "shelljs": "^0.8.5",
    "uuid": "^9.0.1"
  },
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "strict": true,
    "target": "es6",
    "module": "commonjs",
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  }
}
